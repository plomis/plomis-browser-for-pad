{
  "name": "PlomisViewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "react-native run-ios --simulator 'iPad Pro (9.7-inch) (12.2)'",
    "dev-android": "react-native run-android --appIdSuffix \"debug\"",
    "dev-android-staging": "react-native run-android --variant releaseStaging",
    "build-android": "cd android && ./gradlew assembleRelease",
    "install-android": "adb install -r ./android/app/releaseStaging/app-releaseStaging.apk",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.9",
    "events": "^3.0.0",
    "flystore": "^1.1.2",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-code-push": "^5.6.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-orientation-locker": "^1.1.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.5.0",
    "react-native-webview": "^5.12.0",
    "react-navigation": "^3.11.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-promise-react": "^1.0.4",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "flow-bin": "^0.101.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
