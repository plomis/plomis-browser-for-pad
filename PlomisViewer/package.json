    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-import": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-config-promise-react": "^1.0.4",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "flow-bin": "^0.101.0",
