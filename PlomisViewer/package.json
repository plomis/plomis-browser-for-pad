{
  "name": "PlomisViewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios --simulator 'iPad Pro (12.9-inch) (3rd generation) (12.2)'",
    "android": "react-native run-android --appIdSuffix \"debug\"",
    "dev-android-staging": "react-native run-android --variant releaseStaging",
    "build-android": "cd android && ./gradlew assembleRelease",
    "install-android": "adb install -r ./android/app/build/outputs/apk/release/app-release.apk",
    "install-android-release": "adb install -r ./android/app/release/app-release.apk",
    "install-android-staging": "adb install -r ./android/app/releaseStaging/app-releaseStaging.apk",
    "test": "jest"
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.9",
    "@react-native-community/async-storage": "^1.5.1",
    "@react-native-community/blur": "^3.3.1",
    "events": "^3.0.0",
    "expo": "^34.0.3",
    "flystore": "^1.1.2",
    "json5": "^2.1.0",
    "omit.js": "^1.0.2",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-app-store-review": "0.0.3",
    "react-native-appstore-version-checker": "^2.7.2",
    "react-native-code-push": "^7.0.5",
    "react-native-device-info": "^2.3.1",
