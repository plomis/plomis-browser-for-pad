{
  "name": "PlomisBrowserHD",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "react-native run-ios --simulator 'iPad Pro (9.7-inch) (12.2)'",
    "dev-android": "react-native run-android",
    "build-android": "cd android && ./gradlew assembleRelease",
    "build-android-test": "cd android && ./gradlew installRelease",
    "dev-android-release": "react-native run-android --variant release",
    "dev-android-staging": "react-native run-android --variant releaseStaging",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "install-android": "adb install -r ./android/app/build/outputs/apk/pro/release/app-pro-release.apk",
    "flow": "flow",
    "test": "jest"
  },
  "dependencies": {
    "events": "^3.0.0",
    "flystore": "^1.1.2",
    "react": "16.8.3",
    "react-native": "0.62.3",
    "react-native-code-push": "^5.6.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-orientation": "^3.1.3",
    "react-native-orientation-locker": "^1.1.5",
    "react-native-reanimated": "^1.0.1",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "^6.4.2",
    "react-native-webview": "^5.11.0",
    "react-navigation": "^3.9.1",
    "react-navigation-animated-switch": "^0.2.0",
    "urijs": "^1.19.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
